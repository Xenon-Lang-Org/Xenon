let i: mut i32 = 2;

fn is_prime(nb: i32) -> i32
{
    if (nb == 0 || nb == 1) {
        return 0;
    }
    while (i < nb) {
        if (nb % i == 0) {
            return 0;
        }
        i = i + 1;
    }
    return 1;
}

