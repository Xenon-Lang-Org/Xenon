fn sum_even(start: i32, finish: i32) -> i32
{
    let is_even: i32 = (start % 2) == 0;
    let sum: mut i32 = 0;

    if (start > finish) {
        return 0;
    }
    if (is_even) {
        sum = start;
    }
    return sum + sum_even(start + 1, finish);
}

