fn pow(n: i32, p: i32) -> i32
{
    let i: mut i32 = 1;
    let res: mut i32 = n;

    if (p == 0) {
        return 1;
    }
    if (p == 1) {
        return n;
    }
    while (i < p) {
        res = res * n;
        i = i + 1;
    }
    return res;
}

